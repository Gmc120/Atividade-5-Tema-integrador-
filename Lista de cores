#include <stdio.h>

int main() {
    // Declaração e inicialização de um vetor de strings com cores
    char cores[][20] = {"Vermelho", "Verde", "Azul", "Amarelo", "Roxo"};

    // Impressão das cores
    printf("Lista de Cores:\n");
    for (int i = 0; i < 5; i++) {
        printf("%d. %s\n", i+1, cores[i]);
    }

    return 0;
}
